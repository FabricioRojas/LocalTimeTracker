{
	"name": "vstimetracker",
	"displayName": "vsTimeTracker",
	"description": "",
	"publisher": "FabricioRojas",
	"repository": "https://github.com/FabricioRojas/vsTimeTracker",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.getTime",
				"title": "View stats"
			},
			{
				"command": "extension.initTimer",
				"title": "Init Timer"
			},
			{
				"command": "extension.updateStatusTimer",
				"title": "Pause/Start timer"
			},
			{
				"command": "extension.clearStatsRestartTimer",
				"title": "Clear stats and restart the timer"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"anychart": "^8.6.0"
	}
}
